namespace testapi.flatbuffers;

table Simple {
  n: int;
  s: string;
}

table FiveFields {
  n1: int;
  s1: string;
  n2: long;
  s2: string;
  n3: double;
}

table ManyTables {
  n: int;
  x: Simple;
  y: Simple;
  z: Simple;
}

enum Color:ubyte { Red = 0, Green, Blue = 2, Gray = 5, Black = 8 }

union U { UA, UB }
table UA { x: string; }
table UB { y: int; }

table UnionByteBool {
  color: Color;
  color2: Color = Blue;
  uni1: U;
  uni2: U;
  uni3: U;
  uni4: U;
  boo: bool;
}

table Vectors {
  w: [int];
  x: [int];
  y: [string];
  z: [long];
}

struct RegularStruct {
  x: int;
  y: uint;
}

struct IrregularStruct {
  w: int;
  x: ubyte;
  y: long;
  z: bool;
}

struct WithNestedStruct {
  x: RegularStruct;
  y: IrregularStruct;
}

struct EmptyStruct {}

struct EmptyStructWithEmptyStruct {
  x: EmptyStruct;
}

struct EmptyStructWithEmptyStructAndMore {
  x: int32;
  y: EmptyStruct;
  z: int32;
}

table EmptyStructs {
  w: EmptyStruct;
  x: [EmptyStruct];
  y: EmptyStructWithEmptyStruct;
  z: EmptyStructWithEmptyStructAndMore;
}

table Structs {
  w: RegularStruct;
  x: RegularStruct;
  y: IrregularStruct;
  z: WithNestedStruct;
}

table VectorOfTables {
  xs: [Simple];
}

